    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@pixi/app": "^5.3.10",
    "@pixi/core": "^5.3.10",
    "@pixi/interaction": "^5.3.10",
    "@pixi/ticker": "^5.3.10",
    "@tensorflow/tfjs": "^3.8.0",
